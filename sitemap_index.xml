<?xml version="1.0" encoding="UTF-8"?><?xml-stylesheet type="text/xsl" href="{{site.url | remove_first: 'https:' | remove_first: 'http:' }}/contents/addons/w-seo/css/main-sitemap.xsl"?>
<sitemapindex xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">{% assign num = site.posts | size %}{% assign dposts = site.posts | reverse %}{% assign nump = num | divided_by: site.sitemap_limit | ceil %}{% for i in (1..nump) %}{% unless forloop.last %}
    <sitemap>{% assign vse = site.sitemap_limit | times: i | round %}{% assign vsa = vse | minus: 1 | round %}
        <loc>{{site.url}}/post-sitemap{{i}}.xml</loc>
        <lastmod>{{ dposts[vsa].date | date_to_xmlschema }}</lastmod>
    </sitemap>{% endunless %}{% if forloop.last %}
    <sitemap>
        <loc>{{site.url}}/post-sitemap{{i}}.xml</loc>
        <lastmod>{{ site.posts.first.date | date_to_xmlschema }}</lastmod>
    </sitemap>{% endif %}{% endfor %}
    <sitemap>
        <loc>{{site.url}}/page-sitemap.xml</loc>
        <lastmod>{{ site.posts.first.date | date_to_xmlschema }}</lastmod>
    </sitemap>
</sitemapindex>
